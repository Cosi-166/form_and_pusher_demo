<div class="col-md-offset-1 col-md-4">
  <h1>Search</h1>
    <%= form_tag(do_search_items_path, method: "get", role: "form") do %>
      <div class="form-group">
        Items that [match] [all] of the following conditions
      </div>
      <div class="form-group">
        <label>Category:</label>
        <%= select_tag(:catgegory_id, options_for_select(@categories), class: "form-control") %>
      </div>
      <div class="form-group">
        <label>Title</label>
        <%= text_field_tag(:title, "", class: "form-control") %>
      </div>
      <p><%= submit_tag("Search") %>
    <% end %>
</div>