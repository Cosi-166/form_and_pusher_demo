<h1>Search Form</h1>
<%= form_tag(search_path, method: "get") do %>
  <%= label_tag(:title, "Title:") %>
  <%= text_field_tag(:title) %>
  <%= label_tag(:desc, "Description:") %>
  <%= text_field_tag(:desc) %>
  <%= label_tag(:title, "Type:") %>
  <%= text_field_tag(:type) %>
  <%= submit_tag("Search Now!") %>
<% end %>